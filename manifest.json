{
    "name": "SC",
    "description": "An extension for easier redirect to target page!",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "run_at": "document_start",
            "js": ["js/event.js"]
        }
    ],
    "options_page": "html/options.html",
    "icons": {
        "32": "images/link-32.png",
        "64": "images/link-64.png",
        "72": "images/link-72.png",
        "128": "images/link-128.png"
    },
    "permissions": [
      "storage"
    ],
    "manifest_version": 2,
    "version": "2.0"
}
